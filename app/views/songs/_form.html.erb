<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <!-- <%= f.text_field :artist_name %> -->
  <% if @song.artist %>
  <%= @song.artist.name %>
  <% else %>
  <%= artist_select(@song.artist, @song) %>
  <% end %>
  <br />

  <%= f.submit %>
<% end %>
